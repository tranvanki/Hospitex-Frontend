# Environment Variables Template
# Copy this file to .env.local and update with your local configuration

# Backend API Configuration
VUE_APP_API_BASE_URL=https://hospitex-backend-8.onrender.com

# Application Configuration
VUE_APP_NAME=Hospitex System
VUE_APP_VERSION=1.0.0

# Development Configuration
VUE_APP_DEBUG=true
VUE_APP_LOG_LEVEL=debug

# Authentication Configuration
VUE_APP_TOKEN_STORAGE_KEY=token
VUE_APP_SESSION_TIMEOUT=3600000

# API Configuration
VUE_APP_API_TIMEOUT=10000
VUE_APP_MAX_FILE_SIZE=5242880

# Feature Flags
VUE_APP_ENABLE_LOGGING=true
VUE_APP_ENABLE_ANALYTICS=false
